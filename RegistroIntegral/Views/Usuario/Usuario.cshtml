
<div class="mantenimiento-container">

    <div class="row">
        <div class="col-lg-12 mantenimiento-titulo">
            <h2>Usuario <small>Mantenimiento</small></h2>
        </div>
    </div>

    <div class="row filterOption-buttoms">
        <div class="col-lg-4 text-left">
            <button type="button" class="btn btn-success btn-sm" data-toggle="modal" data-target="#myModal">Agregar  <i class="fa fa fa-plus"></i></button>
            <button type="button" class="btn btn-warning btn-sm">Imprimir <i class="=fa fa fa-print"></i></button>
        </div>
        <div class="col-lg-4 col-md-offset-4">
            <div class="input-group">
                <input type="text" class="form-control input-sm">
                <div class="input-group-btn">
                    <button tabindex="-1" class="btn btn-default btn-sm" type="button">Buscar</button>
                    <button tabindex="-1" data-toggle="dropdown" class="btn btn-default btn-sm dropdown-toggle" type="button">
                        <span class="caret"></span>
                        <span class="sr-only">Toggle Dropdown</span>
                    </button>
                    <ul class="dropdown-menu pull-right">
                        <li><a href="#">Codigo</a></li>
                        <li><a href="#">Usuario</a></li>
                        <li><a href="#">Nombre</a></li>


                    </ul>
                </div>
            </div>

        </div>
    </div>
        @*<div class="col-lg-12">*@
            <div class="modal fade " tabindex="-1" role="dialog" aria-labelledby="myModalLabel" id="myModal" aria-hidden="true">
                <div class="modal-dialog modal-lg">
                    <div class="modal-content formcenter">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                            <h3 class="modal-title"><i class="fa fa-user"></i> Nuevo Usuario</h3>
                        </div>

                        <div class="modal-body form-horizontal">
                            <div class="form-group">
                                <label class="col-lg-2 control-label text-right">Nombre:</label>
                                <div class="col-lg-10">
                                    <input id="aula_nombre" onkeypress="return letra(event)" onkeyup="this.value = this.value.toUpperCase();" type="text" class="form-control" placeholder="Ingrese Nombre" autofocus required>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-lg-2 control-label text-right">Usuario:</label>
                                <div class="col-lg-10">
                                    <input id="usuario_user" type="text" class="form-control" placeholder="Ingrese Usuario" autofocus required>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-lg-2 control-label text-right">Clave:</label>
                                <div class="col-lg-10">
                                    <input id="usuario_clave" type="text" class="form-control" placeholder="Ingrese Clave" autofocus required>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-lg-2 control-label text-right">Tipo:</label>
                                <div class="col-lg-10">
                                    <select id="usuario_tipo" class="form-control"></select>
                                </div>
                            </div>

                        </div>

                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal"><i class="fa fa-times-circle"></i> Cerrar</button>
                            <button id="btnsave" type="submit" class="btn btn-primary"><i class="fa fa-floppy-o"></i> Guardar</button>
                        </div>
                    </div>
                </div>
            </div>
        @*</div>*@

        <div class="col-lg-4">

        </div>

        
  


